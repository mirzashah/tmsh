  <joint name="camera1_joint" type="fixed">
    <origin rpy="0 -0.994837673636768 0" xyz="0.04726 0.0175 0.33805"/>
    <parent link="base_link"/>
    <child link="camera1_link"/>
  </joint>
  <link name="camera1_link">
  </link>
